{
  "name": "@realms-world/eslint-config",
  "private": true,
  "version": "0.3.0",
  "type": "module",
  "exports": {
    "./base": "./base.js",
    "./nextjs": "./nextjs.js",
    "./react": "./react.js"
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "format:write": "prettier --check --write . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^14.2.5",
    "eslint-config-turbo": "^2.0.12",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "rc",
    "typescript-eslint": "rc-v8"
  },
  "devDependencies": {
    "@realms-world/prettier-config": "workspace:*",
    "@realms-world/tsconfig": "workspace:*",
    "eslint": "^9.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "prettier": "@realms-world/prettier-config"
}
