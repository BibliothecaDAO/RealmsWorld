{
  "name": "siws",
  "version": "0.0.1",
  "private": true,
  "description": "Sign-In with Starknet helper library",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "directories": {
    "lib": "src"
  },
  "scripts": {
    "dev": "tsup",
    "test": "vitest --passWithNoTests",
    "build": "tsup",
    "gcp-build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --ignore-path .gitignore .",
    "format:write": "prettier --ignore-path .gitignore --write ."
  },
  "author": "Nethermind",
  "license": "Apache-2.0",
  "keywords": [
    "starknet",
    "sign-in",
    "sign-in-with-starknet",
    "siws"
  ],
  "homepage": "https://github.com/nethermindeth",
  "dependencies": {
    "starknet": "^6.9.0",
    "uri-js": "^4.4.1",
    "valid-url": "^1.0.9",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@realms-world/eslint-config": "*",
    "@realms-world/prettier-config": "*",
    "@realms-world/tsconfig": "*",
    "eslint": "^9.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "tsdx": "^0.14.1",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "ethers": "^5.6.8 || ^6.0.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nethermindeth/signin-with-starknet.git"
  },
  "bugs": {
    "url": "https://github.com/spruceid/signin-with-starknet/issues"
  }
}
