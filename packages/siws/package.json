{
  "name": "siws",
  "version": "0.0.1",
  "private": true,
  "description": "Sign-In with Starknet helper library",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "directories": {
    "lib": "src"
  },
  "scripts": {
    "test": "jest",
    "dev": "tsup",
    "build": "tsup",
    "gcp-build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "author": "Nethermind",
  "license": "Apache-2.0",
  "keywords": [
    "starknet",
    "sign-in",
    "sign-in-with-starknet",
    "siws"
  ],
  "homepage": "https://github.com/nethermindeth",
  "dependencies": {
    "starknet": "^6.11.0",
    "uri-js": "^4.4.1",
    "valid-url": "^1.0.9",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.3.0",
    "@realms-world/eslint-config": "workspace:*",
    "@realms-world/prettier-config": "workspace:*",
    "@realms-world/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.2",
    "tsdx": "^0.14.1"
  },
  "peerDependencies": {
    "ethers": "^5.6.8 || ^6.0.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nethermindeth/signin-with-starknet.git"
  },
  "bugs": {
    "url": "https://github.com/spruceid/signin-with-starknet/issues"
  }
}
